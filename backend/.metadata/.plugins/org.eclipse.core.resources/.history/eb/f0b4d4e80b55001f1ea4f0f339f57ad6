package com.Contabilidad.AvaliaNFs;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.autoconfigure.domain.EntityScan;
import org.springframework.data.jpa.repository.config.EnableJpaRepositories;

@SpringBootApplication
@EnableJpaRepositories(basePackages = "com.Contabilidad.AvaliaNFs.repositories")
@EntityScan(basePackages = "com.Contabilidad.AvaliaNFs.domain")
public class AvaliaNFsApplication {

	public static void main(String[] args) {
		SpringApplication.run(AvaliaNFsApplication.class, args);
	}

}
